	local FindFirstChild_ret4 = t_ExclusiveShop.Tabs:FindFirstChild(p9)
	t_ExclusiveShop.Gui.Enabled = true
	UseTab(FindFirstChild_ret4)
end)
_L.Signal.Fired("Clicked Shop Button"):Connect(function()
	UpdateShopButton()
end)
