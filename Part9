			v189.farming = false
			v189.target = nil
			if v189.selectionFx then
				v189.selectionFx()
				v189.selectionFx = nil
			end
		end
		v_u_2.hasPetRidingLocal = true
		v_u_1.Network.Fire("Set Riding Pet", v_u_2.petRidingLocal)
	end
end)
