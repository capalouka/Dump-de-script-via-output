		if typeof(key6) ~= "string" then continue end
		v12[tonumber(key6)] = val6;
		v12[key6] = nil;
	end
	local Map3 = m_Library.WorldCmds.GetMap();
	local BoothSpawns = Map3:FindFirstChild("BoothSpawns");
