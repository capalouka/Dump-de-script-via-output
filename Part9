		child8.CellSize = UDim2.new(child8.CellSize.X.Scale, child8.CellSize.X.Offset, 0, child8.AbsoluteCellSize.Y + 2);
	end
	p4.Size = UDim2.new(p4.Size.X.Scale, p4.Size.X.Offset, 0, v18);
end
