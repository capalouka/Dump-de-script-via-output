		if v_u_2.petRidingLocal == v189.spawned.uid and not v_u_1.Variables.UsingHoverboard and v189.target == v189.spawned.owner then
			v_u_2.petRidingLocal = nil
			v_u_2.destroyByPlayer(v189.spawned.owner)
