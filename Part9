        local v633 = v592.PaddingLeft
        v606 = v633.Offset
        v606 = v592.PaddingRight
        v608 = (v606 - v588.ScrollBarThickness - v606 - v606.Offset) / v629 - v590.CellPadding.X.Offset
        v606 = (v608) * v630
